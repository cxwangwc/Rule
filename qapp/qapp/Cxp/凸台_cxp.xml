step2.setValue('name4', CONCATENATE(['规格:', str(L), '   ', tostr(L4), '/', tostr(M4)]))
step2.setValue('name5', '双端面磨')

#设置第三个工序值
step3.setValue('name1', '3')
step3.setValue('name2', '切片')
