其他说明 = '其他说明********'
涂层种类 = '环保彩锌'
耐蚀性 = '盐雾48h'
涂层厚度 = '5-10μm'
毛坯外形尺寸 = str(H15) + '*' + str(H16) + '*' + str(H17)]]>
			</Statement>
